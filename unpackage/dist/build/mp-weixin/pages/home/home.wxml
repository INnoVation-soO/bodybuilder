<view class="container"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><view data-event-opts="{{[['tap',[['imageClick',[i]]]]]}}" class="swiper-item" bindtap="__e"><image src="{{item.image_src}}"></image></view></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['navClickHandler',['$0'],[[['navList','',i]]]]]]]}}" class="nav-item" bindtap="__e"><image class="nav-img" src="{{item.image_src}}"></image><text class="list-text" style="{{'color:'+(item.color)+';'}}">{{item.txt}}</text></view></block></view><view class="floor-list"><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="floor-item"><text class="floor-title" style="{{'color:'+(item.color)+';'}}">{{item.title}}</text><view class="floor-img-box"><navigator class="left-img-box" url="{{item.url+item.id}}"><image style="{{'width:'+(item.left_img.wid+'rpx')+';'+('height:'+(item.left_img.heigh+'rpx')+';')}}" src="{{item.left_img.img_src}}"></image></navigator><view class="right-img-box"><block wx:for="{{item.right_img}}" wx:for-item="item2" wx:for-index="i2" wx:key="i2"><view class="right-img-item"><navigator url="{{item.url+item2.id}}"><image style="{{'width:'+(item2.wid+'rpx')+';'+('height:'+(item2.heigh+'rpx')+';')}}" src="{{item2.img_src}}"></image></navigator></view></block></view></view></view></block></view></view>